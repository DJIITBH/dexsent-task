<link name="triangular_mount">

  <!-- Inertial properties -->
  <inertial>
    <!-- Center of mass (adjust if STL origin differs) -->
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <mass value="25.0"/>
    <inertia
      ixx="0.58"
      ixy="0.0"
      ixz="0.0"
      iyy="0.96"
      iyz="0.0"
      izz="0.65"/>
  </inertial>

  <!-- Visual -->
  <visual>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <geometry>
      <mesh filename="file://$(find ur_description_pkg)/meshes/prism/triangular.stl"
            scale="0.001 0.001 0.001"/>
    </geometry>
    <!-- <material name="grey">
      <color rgba="0.6 0.6 0.6 1.0"/>
    </material> -->
  </visual>

  <!-- Collision -->
  <collision>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <geometry>
      <mesh filename="file://$(find ur_description_pkg)/meshes/prism/triangular.stl"
            scale="0.001 0.001 0.001"/>
    </geometry>
  </collision>

</link>

    
