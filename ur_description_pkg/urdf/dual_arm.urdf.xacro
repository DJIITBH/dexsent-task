    <joint name="support_to_world" type="fixed">
        <parent link="world"/>
        <child link="triangular_mount"/>
        <origin xyz="0 0 0" rpy="0 0 0"/>
    </joint>

    <joint name="left_arm_mount" type="fixed">
        <parent link="triangular_mount"/>
        <child link="base_link_l"/>
        <origin
            xyz="0.11 0 0.22"
            rpy="0 0.75 0"/>
    </joint>

    <joint name="right_arm_mount" type="fixed">
        <parent link="triangular_mount"/>
        <child link="base_link_r"/>
        <origin
            xyz="-0.11 0 0.22"
            rpy="0 -0.75 0"/>
    </joint>


