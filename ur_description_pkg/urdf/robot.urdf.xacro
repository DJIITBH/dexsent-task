<?xml version="1.0" ?>

<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="robot_arm">
    <xacro:include filename="$(find ur_description_pkg)/urdf/ros2_control.xacro" />
    <!-- <xacro:include filename="$(find robotiq_description)/urdf/robotiq_2f_85_gripper.urdf.xacro"/> -->
    <xacro:include filename="$(find robotiq_description)/urdf/robotiq_2f_85_macro.urdf.xacro" />
    <xacro:arg name="use_fake_hardware" default="false" />
    <xacro:arg name="com_port" default="/dev/ttyUSB0" />

    <link name="world">
    </link>
    

   <link name="base_link">
  <visual>
    <origin rpy="0 0 0" xyz="0 0 0"/>
    <geometry>
      <mesh filename="file://$(find ur_description_pkg)/meshes/crx10ial/visual/base_link.stl"/>
    </geometry>
    <material name="">
      <color rgba="0.239 0.239 0.239 1.0"/>
    </material>
  </visual>
  <collision>
    <origin rpy="0 0 0" xyz="0 0 0"/>
    <geometry>
      <mesh filename="file://$(find ur_description_pkg)/meshes/crx10ial/collision/base_link.stl"/>
    </geometry>
  </collision>
  <inertial>
    <mass value="15.0"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <inertia ixx="0.5" ixy="0.0" ixz="0.0"
             iyy="0.5" iyz="0.0"
             izz="0.5"/>
  </inertial>
</link>

